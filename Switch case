#include<stdio.h>
main()
{
	printf(" Select the food item from the given food menu.\n THE FOOD MENU :\n1) French Fries (Rs 99) \n2) Pizza (Rs 239) \n3) Sandwich (Rs 149) \n4) Pasta (Rs 179) \n5) Burger (Rs 129) \n");
	int f=0;
	scanf("%d",&f);
	
	switch(f)
	{
		case 1:
			printf(" Food Item - French Fries\n Price - Rs 99\n");
			break;
		
		case 2:
			printf(" Food Item - Pizza\n Price - Rs 239\n");
			break;
		
		case 3:
			printf(" Food Item - Sandwich \n Price - Rs 149\n");
			break;
		
		case 4:
			printf(" Food Item - Pasta\n Price - Rs 179\n");
			break;
		
		case 5:
			printf(" Food Item - Burger\n Price - Rs 120\n");
			break;
			
		default : printf(" Invalid choice.");
	}
	return 0;
}
